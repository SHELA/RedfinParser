{
    "name": "shela/redfin-parser",
    "description": "Parser for redfin.com",
    "type": "library",
    "keywords": ["redfin", "parser"],
    "homepage": "https://github.com/shela/redfin-parser",
    "minimum-stability": "dev",
    "license": [
        "RPL-1.5",
        "proprietary"
    ],
    "require": {
        "guzzlehttp/guzzle": "^6.3",
        "php": "^7.0",
        "fzaninotto/faker": "^1.8",
        "jabranr/csv-parser": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Shela\\RedfinParser\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Vladyslav Sheludko",
            "email": "me@zenzenzen.net"
        }
    ],
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.11.0"
    },
    "scripts": {
        "codestyle": [
            "php-cs-fixer fix --config=.php_cs.dist --allow-risky yes"
        ]
    }
}
